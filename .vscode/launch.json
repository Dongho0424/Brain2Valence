{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "reg_brain3d: train",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--exec_mode", "train",
                "--all-subjects",
                "--wandb-name",
                "0215_resnet18_mae_1",
                "--model-name",
                "all_subjects_resnet18_mae_1",
                "--wandb-project",
                "Brain2Valence",
                "--wandb-entity",
                "donghochoi",
                "--model",
                "resnet18",
                "--task-type",
                "reg",
                "--data",
                "brain3d",
                "--epochs",
                "100",
                "--batch-size",
                "32",
                "--lr",
                "1e-5",
                "--weight-decay",
                "0.001",
                "--seed",
                "42",
                "--criterion",
                "mae"
            ],
            "console": "integratedTerminal",
            "env": {
                "CUDA_VISIBLE_DEVICES": "3"
            }
        },
        {
            "name": "reg_brain3d: predict",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--exec_mode", "predict",
                "--all-subjects",
                "--wandb-name", "0215_resnet18_mae_1",
                "--model-name", "all_subjects_resnet18_mae_1",
                "--wandb-project", "Brain2Valence",
                "--wandb-entity", "donghochoi",
                "--model", "resnet18",
                "--task-type", "reg",
                "--data", "brain3d",
                "--best"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "3"
            }
        }
    ]
}
