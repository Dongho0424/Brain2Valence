{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Train Brain2Valence",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--exec_mode", "train",
                "--wandb-name", "test",
                "--wandb-project", "Brain2Valence",
                "--wandb-entity", "donghochoi",
                "--epochs", "100",
                "--batch-size", "32",
                "--lr", "1e-4",
                "--weight-decay", "0.0001",
                "--seed", "42",
                "--criterion", "mae"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "3"
            }
        }
    ]
}
